   "source": [
    "#Set seed\n",
    "np.random.seed(1231)\n",
    "\n",
    "#Create praeto distribution\n",
    "pareto = np.random.pareto(1,100)\n",
    "\n",
    "#Plot histogram of pareto distribution\n",
    "plt.hist(pareto)\n",
    "\n",
    "#Create solidl ine at mean\n",
    "plt.axvline(pareto.mean(),color='b',linestyle='solid',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev below mean\n",
    "plt.axvline(pareto.mean()-pareto.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev above mean\n",
    "plt.axvline(pareto.mean()+pareto.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Show plot\n",
    "plt.show()"
   ]
