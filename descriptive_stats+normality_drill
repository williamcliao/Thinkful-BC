   "source": [
    "#Set seed\n",
    "np.random.seed(1231)\n",
    "\n",
    "#Create exponential distribution\n",
    "exponential = np.random.exponential(1,100)\n",
    "\n",
    "#Plot histogram of exponential distribution\n",
    "plt.hist(exponential)\n",
    "\n",
    "#Create solid line at mean\n",
    "plt.axvline(exponential.mean(),color='b',linestyle='solid',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev below mean\n",
    "plt.axvline(exponential.mean()-exponential.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev above mean\n",
    "plt.axvline(exponential.mean()+exponential.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Show plot\n",
    "plt.show()"
   ]
  },
