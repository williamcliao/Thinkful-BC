    "#Sort rayleigh distribution in ascending order\n",
    "rayleigh.sort()\n",
    "\n",
    "#Create normal distribution\n",
    "norm = np.random.normal(0,1,100)\n",
    "\n",
    "#Sort normal distribution in ascending order\n",
    "norm.sort()\n",
    "\n",
    "#Plot normal and rayleigh distributions to create QQ plot\n",
    "plt.plot(norm, rayleigh, \"o\")\n",
    "\n",
    "#Show plot\n",
    "plt.show()\n"
