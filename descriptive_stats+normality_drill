   "source": [
    "#Set seed\n",
    "np.random.seed(1231)\n",
    "\n",
    "#Create normal distribution 1\n",
    "var1 = np.random.normal(5,.5, 100)\n",
    "\n",
    "#Create normal distribution 2\n",
    "var2 = np.random.normal(10,1, 100)\n",
    "\n",
    "#Combine both normal distributions, assign to 'total\n",
    "combined = var1+var2\n",
    "\n",
    "#Plot histogram \n",
    "plt.hist(total)\n",
    "\n",
    "#Create solid line at mean\n",
    "plt.axvline(combined.mean(),color='b',linestyle='solid',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev below mean\n",
    "plt.axvline(combined.mean()-total.std(), linestyle='dashed',color='red',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev above mean\n",
    "plt.axvline(combined.mean()+total.std(), linestyle='dashed',color='red',linewidth=2)\n",
    "\n",
    "#Show plot\n",
    "plt.show()"
   ]
  },
