    "#Sort pareto distribution in ascending order\n",
    "pareto.sort()\n",
    "\n",
    "#Create normal distribution\n",
    "norm = np.random.normal(0,1,100)\n",
    "\n",
    "#Sort normal distribution in ascending order\n",
    "norm.sort()\n",
    "\n",
    "#Plot normal distributon and pareto distribution to create QQ plot\n",
    "plt.plot(norm, pareto, \"o\")\n",
    "\n",
    "#Show plot\n",
    "plt.show()\n"
