   "source": [
    "#Set seed\n",
    "np.random.seed(1231)\n",
    "\n",
    "#Create rayleigh distribution\n",
    "rayleigh = np.random.rayleigh(5,100)\n",
    "\n",
    "#Plot histogram of rayleigh distribution\n",
    "plt.hist(rayleigh)\n",
    "\n",
    "#Create solid line at mean\n",
    "plt.axvline(rayleigh.mean(),color='b',linestyle='solid',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev below mean\n",
    "plt.axvline(rayleigh.mean()-rayleigh.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev above mean\n",
    "plt.axvline(rayleigh.mean()+rayleigh.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Show plot\n",
    "plt.show()"
   ]
  },
