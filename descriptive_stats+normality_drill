    "#Sort poisson distribution in ascending order\n",
    "poisson.sort()\n",
    "\n",
    "#Create normal distribution\n",
    "norm = np.random.normal(0,1,100)\n",
    "\n",
    "#Sort normal distribution in ascending order\n",
    "norm.sort()\n",
    "\n",
    "#Plot normal distribution and poisson distribution to create QQ plot\n",
    "plt.plot(norm, poisson, \"o\")\n",
    "\n",
    "#Show QQ plot\n",
    "plt.show()\n"
