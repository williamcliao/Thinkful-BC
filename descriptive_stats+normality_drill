   "source": [
    "#Set seed\n",
    "np.random.seed(1231)\n",
    "\n",
    "#Create histogram of poisson distribution\n",
    "poisson = np.random.poisson(1,100)\n",
    "\n",
    "#Plot poisson distribution\n",
    "plt.hist(poisson)\n",
    "\n",
    "#Create line solid line at mean\n",
    "plt.axvline(poisson.mean(),color='b',linestyle='solid',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev below mean\n",
    "plt.axvline(poisson.mean()-poisson.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev above mean\n",
    "plt.axvline(poisson.mean()+poisson.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Show plot\n",
    "plt.show()"
   ]
  },
