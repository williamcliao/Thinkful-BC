    "#Sort uniform distribution in ascending order\n",
    "uniform.sort()\n",
    "\n",
    "#Create normal distribution\n",
    "norm = np.random.normal(0,1,100)\n",
    "\n",
    "#Sort normal distribution in acsending order\n",
    "norm.sort()\n",
    "\n",
    "#Plot normal and uniform distribution to create QQ plot\n",
    "plt.plot(norm, uniform, \"o\")\n",
    "\n",
    "#Show distribution\n",
    "plt.show()\n"
