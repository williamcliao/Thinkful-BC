   "source": [
    "#Set seed\n",
    "np.random.seed(1231)\n",
    "\n",
    "#Create uniform distribution\n",
    "uniform = np.random.uniform(1,1,100)\n",
    "\n",
    "#Plot histogram of uniform distribution\n",
    "plt.hist(uniform)\n",
    "\n",
    "#Create solid line at mean\n",
    "plt.axvline(uniform.mean(),color='b',linestyle='solid',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev below mean\n",
    "plt.axvline(uniform.mean()-uniform.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev above mean\n",
    "plt.axvline(uniform.mean()+uniform.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Show plot\n",
    "plt.show()"
   ]
  },
