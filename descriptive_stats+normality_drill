   "source": [
    "#Set seed\n",
    "np.random.seed(1231)\n",
    "\n",
    "#Create logistic distribution\n",
    "logistic = np.random.logistic(5,5,100)\n",
    "\n",
    "#Plot histogram of logistic distribution\n",
    "plt.hist(logistic)\n",
    "\n",
    "#Create solid line at mean\n",
    "plt.axvline(logistic.mean(),color='b',linestyle='solid',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev below mean\n",
    "plt.axvline(logistic.mean()-logistic.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Create dotted line 1 st dev above mean\n",
    "plt.axvline(logistic.mean()+logistic.std(),color='r',linestyle='dashed',linewidth=2)\n",
    "\n",
    "#Show plot\n",
    "plt.show()"
   ]
  },
