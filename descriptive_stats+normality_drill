    "#Sort logistic distribution in ascending order\n",
    "logistic.sort()\n",
    "\n",
    "#Create normal distribution\n",
    "norm = np.random.normal(0,1,100)\n",
    "\n",
    "#Sort normal distribution in acsendig order\n",
    "norm.sort()\n",
    "\n",
    "#Plot normal and logistic distribution to create QQ plot\n",
    "plt.plot(norm, logistic, \"o\")\n",
    "\n",
    "#Show QQ plot\n",
    "plt.show()\n"
